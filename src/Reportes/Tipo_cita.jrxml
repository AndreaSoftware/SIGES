	<queryString>
		<![CDATA[SELECT
     Tipo_citas."tipo_cita" AS
Tipo_citas_tipo_cita,
     Citas."cita_fecha" AS
Citas_cita_fecha,
     Citas."cita_hora" AS
Citas_cita_hora,
     Cliente_duenio."duenio_nombre" AS
Cliente_duenio_duenio_nombre,
      Cliente_duenio."duenio_apellidos" AS
Cliente_duenio_duenio_apellidos,
      Tipo_cliente."tipo_cliente" AS
Tipo_cliente_tipo_cliente

FROM
     "dbo"."Tipo_citas" Tipo_citas
INNER JOIN "dbo"."Citas" Citas ON Tipo_citas."ID_tipoCita" = Citas."ID_cita"
INNER JOIN "dbo"."Cliente_duenio" Cliente_duenio ON Tipo_citas."ID_tipoCita" = Cliente_duenio."ID_DUI"
INNER JOIN "dbo"."Tipo_cliente" Tipo_cliente ON Tipo_citas."ID_tipoCita" = Tipo_cliente."ID_tipoCliente"
Order by
     Tipo_citas."tipo_cita"]]>
