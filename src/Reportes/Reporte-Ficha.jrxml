	<queryString>
		<![CDATA[SELECT
     Ficha_clinica."Tratamiento" AS Ficha_clinica_Tratamiento,
     Ficha_clinica."Dosis" AS Ficha_clinica_Dosis,
     Ficha_clinica."Frecuencia" AS Ficha_clinica_Frecuencia,
     Ficha_clinica."ID_DUI" AS Ficha_clinica_ID_DUI,
     Ficha_clinica."ID_Mascota" AS Ficha_clinica_ID_Mascota,
     Citas."cita_fecha" AS Citas_cita_fecha,
     Citas."cita_hora" AS Citas_cita_hora,
     Cliente_duenio."duenio_nombre" AS Cliente_duenio_duenio_nombre,
     Cliente_duenio."duenio_apellidos" AS Cliente_duenio_duenio_apellidos
FROM
     "dbo"."Cliente_duenio" Cliente_duenio INNER JOIN "dbo"."Citas" Citas ON Cliente_duenio."ID_DUI" = Citas."ID_DUI"
     INNER JOIN "dbo"."Ficha_clinica" Ficha_clinica ON Cliente_duenio."ID_DUI" = Ficha_clinica."ID_DUI"]]>
