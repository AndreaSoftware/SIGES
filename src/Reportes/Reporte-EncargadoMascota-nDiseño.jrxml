	<queryString>
		<![CDATA[SELECT
     Cliente_duenio."ID_DUI" AS Cliente_duenio_ID_DUI,
     Cliente_duenio."duenio_nombre" AS Cliente_duenio_duenio_nombre,
     Cliente_duenio."duenio_apellidos" AS Cliente_duenio_duenio_apellidos,
     Mascota."nombre_mascota" AS Mascota_nombre_mascota,
     Ficha_clinica."Tratamiento" AS Ficha_clinica_Tratamiento,
     Ficha_clinica."Dosis" AS Ficha_clinica_Dosis,
     Ficha_clinica."Frecuencia" AS Ficha_clinica_Frecuencia,
     Cliente_duenio."duenio_telefono" AS Cliente_duenio_duenio_telefono,
     Cliente_duenio."duenio_domicilio" AS Cliente_duenio_duenio_domicilio,
     Cliente_duenio."duenio_correo" AS Cliente_duenio_duenio_correo,
     Mascota."ID_mascota" AS Mascota_ID_mascota
FROM
     "dbo"."Cliente_duenio" Cliente_duenio INNER JOIN "dbo"."Mascota" Mascota ON Cliente_duenio."ID_DUI" = Mascota."ID_DUI"
     INNER JOIN "dbo"."Ficha_clinica" Ficha_clinica ON Cliente_duenio."ID_DUI" = Ficha_clinica."ID_DUI"
     AND Mascota."ID_mascota" = Ficha_clinica."ID_Mascota"]]>
	</queryString>
